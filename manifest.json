{
    "name": "Odissey Music Extension",
    "version": "1.0",
    "description": "A Spotify music extension for chrome users",
    "manifest_version": 2,
    "background":{
        "page": "background.html",
        "persistent": false
    },
    "icons":{
        "128":"odyssey-logo128.png",
        "48": "odyssey-logo48.png",
        "16": "odyssey-logo16.png"
    },
    "omnibox":{ "keyword": "@odyssey"},
    "browser_action":{
        "default_icon": "odyssey-logo16.png",
        "default_popup": "popup.html"
    }
}